        <ContentElement xsi:type="uma:Task" name="impl_developer_tests" briefDescription="Implement one or more tests that enable the validation of the individual software components through execution." id="_0iL1EMlgEdmt3adZL5Dmdw" orderingGuide="" presentationName="Implement Developer Tests" suppressed="false" isAbstract="false" variabilityType="na">
